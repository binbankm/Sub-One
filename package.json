    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "miniflare": "^4.20250604.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
